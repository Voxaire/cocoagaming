{
	"type": "origins:multiple",
	"name": "Inferno",
	"description": "While on fire, you can fly very slowly, press space to ascend and hold shift to slowly descend",
	"slowfall": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:apply_effect",
			"effect": {
				"effect": "minecraft:slow_falling",
				"duration": 10,
				"amplifier": 2,
				"show_particles": false,
				"show_icon": false
			}
		},
		"condition": {
			"type": "origins:on_fire"
		},
		"cooldown": 0,
		"hud_render": {
			"should_render": false
		},
		"key": {
			"key": "key.sneak",
			"continuous": true
		}
	},
	"levitate": {
		"type": "origins:active_self",
		"entity_action": {
			"type": "origins:if_else",
			"condition": {
				"type": "origins:and",
				"conditions": [{
						"type": "origins:status_effect",
						"effect": "minecraft:levitation",
						"min_amplifier": -5,
						"max_amplifier": -5,
						"inverted": true
					},
					{
						"type": "origins:on_fire"
					},
					{
						"type": "origins:in_block",
						"block_condition": {
							"type": "origins:height",
							"comparison": "<=",
							"compare_to": 200
						}
					}
				]
			},

			"else_action": {
				"type": "origins:clear_effect",
				"effect": "minecraft:levitation"
			},
			"if_action": {
				"type": "origins:apply_effect",
				"effect": {
					"effect": "minecraft:levitation",
					"duration": 10,
					"amplifier": 1,
					"show_particles": false,
					"show_icon": false
				}
			}
		},
		"cooldown": 0,
		"hud_render": {
			"should_render": false
		},
		"key": {
			"key": "key.jump",
			"continuous": true
		}
	}
}